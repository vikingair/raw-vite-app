{
  "name": "raw-vite-app",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fdc-viktor-luft/raw-vite-app"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "pnpm lint:cc && pnpm lint:es && pnpm lint:ts",
    "lint:cc": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/",
    "lint:es": "eslint . --max-warnings 0",
    "lint:fix": "pnpm lint:es --fix",
    "lint:ts": "tsc",
    "test": "vitest --run --coverage"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.0",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.12.0",
    "@testing-library/react": "16.0.1",
    "@types/jest": "29.5.13",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.2",
    "@vitest/coverage-v8": "2.1.2",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "15.10.0",
    "jsdom": "25.0.1",
    "madge": "8.0.0",
    "prettier": "3.3.3",
    "sass": "1.79.4",
    "spy4js": "4.0.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.8.0",
    "vite": "5.4.8",
    "vitest": "2.1.2"
  },
  "engines": {
    "node": ">= 20.13 < 21",
    "pnpm": ">= 9.1.1"
  }
}
